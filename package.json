{
    "name": "rcdb",
    "version": "1.0.0",
    "description": "A simple Revolt Bot for experiments",
    "main": "src/index.js",
    "type": "module",
    "scripts": {
        "start": "node --es-module-specifier-resolution=node --experimental-fetch src/index dist/index.js",
        "test": "nodemon --es-module-specifier-resolution=node --experimental-fetch src/index dist/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/eliyya/rcdb.git"
    },
    "keywords": [
        "revolt",
        "discord",
        "bot",
        "revolt-bot",
        "discord-bot",
        "discord-js",
        "revolt-js"
    ],
    "author": "eliyya",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/eliyya/rcdb/issues"
    },
    "homepage": "https://github.com/eliyya/rcdb#readme",
    "dependencies": {
        "cross-env": "7.0.3",
        "discord.js": "13.6.0",
        "dotenv": "14.3.2",
        "revolt.js": "^5.2.8"
    },
    "prettier": {
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "trailingComma": "none",
        "semi": false,
        "singleQuote": true,
        "tabWidth": 4,
        "printWidth": 160
    }
}
