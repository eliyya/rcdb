    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/eliyya/revolt-bot.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/eliyya/revolt-bot/issues"
    },
    "homepage": "https://github.com/eliyya/revolt-bot#readme",
    "dependencies": {
        "axios": "^0.26.0",
        "cross-env": "7.0.3",
